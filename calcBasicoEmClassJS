    //
    class Calc {
      constructor(numero1, numero2){
        this.num1 = numero1
        this.num2 = numero2
      }
      out(){
        console.log(this.num1, this.num2);
        
        document.querySelector('.root').innerHTML = ` numero eh ( ${this.num1} ) multiplicado por numero2 fica: ( ${this.num1*this.num2} )`
        
         document.querySelector('.principal').setAttribute('style', 'color: #e0a; font-size: 3rem; margin-top: 3rem; font-weight: 900;')
        
      }
    }

    const a = new Calc(5, 6)
    a.out()

    // 5 6
    // o resultado Ã© 5 e 6 separados e tambem vamos escrever o numero1 e multiplica por numero2 onde vai ser 30 escrito no body
